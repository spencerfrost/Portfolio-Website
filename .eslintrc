{
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:vue/vue3-recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["vue", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "vue/html-closing-bracket-newline": [
      "error",
      {
        "singleline": "never",
        "multiline": "always",
      },
    ],
  },
}
